{
  "name": "contributors-cli",
  "version": "0.0.1",
  "description": "A cli that create CONTRIBUTORS.md file or CONTRIBUTORS section in the README.md for all source code services.",
  "main": "lib",
  "typings": "lib",
  "scripts": {
    "start": "npm run dev",
    "build": "tsc --build .",
    "dev": "ts-node src/index.ts",
    "format": "prettier --write .",
    "format:lint": "prettier --check .",
    "lint": "eslint src/**",
    "release": "standard-version",
    "cm": "cz",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "execa": "^5.1.1",
    "prompts": "^2.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/node": "^7.14.9",
    "@babel/plugin-transform-modules-commonjs": "^7.14.5",
    "@babel/preset-env": "^7.14.9",
    "@babel/preset-typescript": "^7.14.5",
    "@types/node": "^16.4.13",
    "@types/prompts": "^2.0.14",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "chalk": "^4.1.2",
    "commander": "^8.1.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.24.0",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "standard-version": "^9.3.1",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5",
    "husky": "^7.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
